{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowDownloadLimitReadWrite",
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem"
            ],
            "Resource": "arn:aws:"
        },
        {
            "Sid": "AllowS3DownloadObject",
            "Effect": "Allow",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::.zip"
        }
    ]
}